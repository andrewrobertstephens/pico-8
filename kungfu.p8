version 27
__lua__
palt(0,false)
palt(12,true)


left=0
right=1
up=0
down=1
baseline=225
gravity=2
enemy_counter=0
grab_guy=0
knife_guy=1
player={
	x=384+60,
	y=baseline,
	walking=false,
	w_index=0,
	direction=left,
	position=up,
	kicking=0,
	punching=0,
	jumping=0,
	speed=2,
}
enemies={}
ticks=0


function _draw()	
	cls(12)
	map(0,0,0,24,64,32)
	draw_player()	
	draw_enemies()
	draw_osd()
end


function _update()
	ticks=ticks+1
	
	if ticks%20==0 then
		new_enemy()
	end
	
	update_player()
	update_enemies()
	update_camera()
end


function collision(rect1,rect2,size1,size2)
	return rect1.x<rect2.x+12 and
   rect1.x+12>rect2.x and
   rect1.y<rect2.y+size2 and
   rect1.y+size1>rect2.y
end


function draw_osd()
	local camera_x = get_camera_x()
	local camera_y = get_camera_y()
	--[[
	rectfill(camera_x,camera_y,camera_x+128,camera_y+24,0)
	print("1-000000",camera_x,camera_y,7)
	print("player:_____",camera_x,camera_y+8,7)
	print("enemy: _____",camera_x,camera_y+16,7)
	]]
	print(tostr(enemy_counter),camera_x,camera_y,7)
end

function get_camera_x()
	return peek2(0x5f28)
end

function get_camera_y()
	return peek2(0x5f2a)
end

function update_camera()
	camera(player.x-60,baseline-66)
	local camera_x = peek2(0x5f28)
	if camera_x<0 then
		camera(0,baseline-66)
	elseif camera_x>384 then
		camera(384,baseline-66)
	end
end


-->8
-- player


function draw_player()
	local sprite=0
	if player.jumping>0 then
		sprite=2
		if player.kicking>8 then
			sprite=38
		elseif player.kicking>0 then
			sprite=6
		elseif player.punching>8 then
			sprite=40
		elseif player.punching>0 then
			sprite=6
		end
	elseif player.position==up then
		if player.kicking>8 then
			sprite=8
		elseif player.kicking>0 then
			sprite=6
		elseif player.punching>8 then
			sprite=12
		elseif player.punching>0 then
			sprite=10
		elseif player.walking==true then
			if player.w_index==3 then
				sprite=2
			else
				sprite=player.w_index*2
			end
		end
	else
		sprite=14
		if player.kicking>8 then
			sprite=42
		elseif player.kicking>0 then
			sprite=14
		elseif player.punching>8 then
			sprite=34
		elseif player.punching>0 then
			sprite=32
		end
	end
	local flip_x=false
	if player.direction==0 then
		flip_x=true
	end
	spr(sprite,player.x,player.y,2,2,flip_x)
end


function update_player()

	if ticks%4==0 then
		player.w_index+=1
		if player.w_index>3 then
			player.w_index=0
		end
	end
	
	if btn(⬅️) and player.jumping==0 then
		player.direction=left
	elseif btn(➡️) and player.jumping==0 then
		player.direction=right
	end
	
	if btn(⬇️) then
		player.position=down
	elseif player.punching==0 and player.kicking==0 then
		player.position=up
	end
	
	if btn(⬆️) and player.y==baseline then
		player.jumping=10
	end
	
	if btn(4) then
		player.kicking=10
	elseif btn(5) then
		player.punching=10
	elseif btn(⬅️) and player.kicking==0 and player.punching==0 then
		player.x-=player.speed
		player.walking=true
	elseif btn(➡️) and player.kicking==0 and player.punching==0 then
		player.x+=player.speed
		player.walking=true
	else
		player.walking=false
		player.w_index=0
	end
	
	-- apply gravity
	if player.jumping>5 then
		player.y-=gravity
	else
		player.y+=gravity
		if player.y>baseline then
			player.y=baseline
		end
	end
	player.kicking-=1
	if player.kicking<0 then
		player.kicking=0
	end
	player.punching-=1
	if player.punching<0 then
		player.punching=0
	end
	player.jumping-=1
	if player.jumping<0 then
		player.jumping=0
	end	
end
