pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
left=0
right=1
up=0
down=1
baseline=473

player={
	x=440,
	y=baseline,
	walking=false,
	w_index=0,
	direction=left,
	position=up,
	kicking=0,
	punching=0,
	jumping=0,
}

ticks=0


function _update()

	ticks=ticks+1
	if ticks%4==0 then
		player.w_index=player.w_index+1
		if player.w_index>3 then
			player.w_index=0
		end
	end

	if btn(⬇️) then
		player.position=down
	elseif player.punching==0 and
			player.kicking==0 then
		player.position=up
	end
	
	if btn(⬆️) and player.y==baseline then
		player.jumping=10
	end

	if btn(4) then
		player.kicking=10
	elseif btn(5) then
		player.punching=10
	elseif btn(⬅️) and 
			player.kicking==0 and
			player.punching==0 then
		player.x=player.x-1
		player.walking=true
		player.direction=left
	elseif btn(➡️) and
			player.kicking==0 and
			player.punching==0 then
		player.x=player.x+1
		player.walking=true
		player.direction=right
	else
		player.walking=false
		player.w_index=0
	end
	
	-- apply gravity
	if player.jumping>5 then
		player.y=player.y-1
	else
		player.y=player.y+1
		if player.y>baseline then
			player.y=baseline
		end
	end
	
	player.kicking=player.kicking-1
	if player.kicking<0 then
		player.kicking=0
	end
	
	player.punching=player.punching-1
	if player.punching<0 then
		player.punching=0
	end
	
	player.jumping=player.jumping-1
	if player.jumping<0 then
		player.jumping=0
	end
	
end


function _draw()	

	local sprite=0
	if player.position==up then
		if player.kicking>8 then
			sprite=8
		elseif player.kicking>0 then
			sprite=6
		elseif player.punching>8 then
			sprite=12
		elseif player.punching>0 then
			sprite=10
		elseif player.walking==true then
			if player.w_index==3 then
				sprite=2
			else
				sprite=player.w_index*2
			end
		end
	else
		sprite=14
		if player.kicking>8 then
			sprite=42
		elseif player.kicking>0 then
			sprite=14
		elseif player.punching>8 then
			sprite=34
		elseif player.punching>0 then
			sprite=32
		end
	end
	
	local flip_x=false
	if player.direction==0 then
		flip_x=true
	end

	cls(0)
	map(0,0,0,0,64,64)
	spr(sprite,player.x,player.y,2,2,flip_x)
	camera(player.x-60,baseline-60)
	local camera_x = peek2(0x5f28)

end
