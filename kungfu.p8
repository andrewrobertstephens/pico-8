-- enemies


function draw_enemies()
	for i,enemy in ipairs(enemies) do
		local index=128
		if enemy.kind==1 then
			index=160
		end
		local sprite=enemy.w_index*2+index
		local flip_x=false
		if enemy.dead==true then
			if enemy.kind==0 then
				sprite=138
			elseif enemy.kind==1 then
				sprtie=172
			end
		end
		if enemy.direction==left then
			flip_x=true
		end
		spr(sprite,enemy.x,enemy.y,2,2,flip_x)
	end
end


function new_enemy()
	local camera_x = peek2(0x5f28)
	enemy={
		y=baseline,
		w_index=0,
		direction=flr(rnd(2)),
		kind=0,
		health=1,
	}
	if enemy.direction==left then
		enemy.x=camera_x+128+8
	else
		enemy.x=camera_x-8
	end
	if enemy_counter>5 then
		enemy.kind=1
		enemy.health=2
		enemy_counter=0
	end
	add(enemies,enemy)
	enemy_counter+=1
end


function update_enemies()

	for i,enemy in ipairs(enemies) do

		if ticks%3==0 then
			enemy.w_index=enemy.w_index+1
			if enemy.w_index>1 then
				enemy.w_index=0
			end
		end
		
		if collision(enemy,player,16,16) and 
				(player.punching>5 or player.kicking>5) and
				player.direction!=enemy.direction then
			enemy.health-=1
			if enemy.health<1 then
				enemy.dead=true
			end
		end
		
		if enemy.dead==true then
			if enemy.direction==right then
				enemy.x-=gravity/2
				enemy.y+=gravity
			else
				enemy.x+=gravity/2
				enemy.y+=gravity
			end

		else

			if enemy.kind==grab_guy then
				if enemy.direction==right then
					enemy.x+=1
				else
					enemy.x-=1
				end
				
			elseif enemy.kind==knife_guy then
				if enemy.direction==right then
					if enemy.x<player.x-20 then
						enemy.x+=1
					end
				else
					if enemy.x>player.x+20 then
						enemy.x-=1
					end
				end
				
			end

		end
		
		if enemy.y>get_camera_y()+128 then
			del(enemies,enemy)
		end

	end

end
