-- pico-8 update
function _update()
 stars_update()
	if scene=="intro" then
	 intro_update()
	elseif scene=="level" then
		level_scene_update()
 elseif scene=="game" then
		game_update()		
	end		
end

-- pico-8 draw
function _draw()
	cls()
	stars_draw()
 if scene=="intro" then
 	intro_draw()
 elseif scene=="level" then
 	level_scene_draw()
 elseif scene=="game" then
  game_draw()
 end   
end

