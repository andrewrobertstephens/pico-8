function explosions_new(x,y,kind)
 local explosion={}
 explosion.x=x
 explosion.y=y
 explosion.index=0
 explosion.kind=kind
 add(explosions,explosion)
end

function explosions_update()
 if ticks%2==0 then
	 for i=#explosions,1,-1 do
	  pe=explosions[i]
	  pe.index=pe.index+1
	  if pe.index>4 then
	   del(explosions,pe)
	  end
	 end
 end
end
